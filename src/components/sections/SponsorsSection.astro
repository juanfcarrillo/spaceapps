---
import AEI_logo from "../../assets/Patrocinadores/AEI_logo.png";
import AMCHAM_logo from "../../assets/Patrocinadores/AMCHAM_logo.png";
import ASU_logo from "../../assets/Patrocinadores/ASU_logo.png";
import BUENAVISTA_logo from "../../assets/Patrocinadores/BUENAVISTA_logo.png";
import CEDIA_logo from "../../assets/Patrocinadores/CEDIA_logo.png";
import ECUACAP_logo from "../../assets/Patrocinadores/ECUACAP_logo.png";
import GEN_SPACE_logo from "../../assets/Patrocinadores/GEN_SPACE_logo.png";
import GUAYAQUIL_SPACE_logo from "../../assets/Patrocinadores/GUAYAQUIL_SPACE_logo.png";
import LEVIATHAN_SPACE_logo from "../../assets/Patrocinadores/LEVIATHAN_SPACE_logo.png";
import MINISTERIO_E_logo from "../../assets/Patrocinadores/MINISTERIO_E_logo.png";
import MINISTERIO_PCEIP_logo from "../../assets/Patrocinadores/MINISTERIO_PCEIP_logo.png";
import MINISTERIO_REMH_logo from "../../assets/Patrocinadores/MINISTERIO_REMH_logo.png";
import MOVISTAR_logo from "../../assets/Patrocinadores/MOVISTAR_logo.png";
import USFQ_logo from "../../assets/Patrocinadores/USFQ_logo.png";
import WORKINGUP_logo from "../../assets/Patrocinadores/WORKINGUP_logo.png";
---
<head>
    <link rel="stylesheet" href="https://unpkg.com/flowbite@1.4.1/dist/flowbite.min.css" />
    <style>
        .sponsors-section img {
            max-width: 100px; /* Reduce el tama√±o de las im√°genes para m√≥viles */
        }

        @media (min-width: 640px) {
            .sponsors-section img {
                max-width: 150px; /* Tama√±o de las im√°genes para pantallas m√°s grandes */
            }
        }
    </style>
</head>
<body>
    <section class="sponsors-section py-10">
        <div class="container mx-auto text-center">
            <h2 class="text-3xl font-bold mb-6 sm:mb-12">Patrocinadores</h2>
            <h3 class="mb-6 sm:mb-10 text-base sm:text-lg">üë®‚ÄçüöÄ Ellos son los pioneros en <strong>impulsar y difundir nuestro mensaje</strong> sobre el crecimiento del <strong>desarrollo espacial</strong> en el Ecuador üåå</h3>
            <div id="default-carousel" class="relative w-full" data-carousel="slide">
                <!-- Carousel wrapper -->
                <div class="relative h-56 overflow-hidden rounded-lg md:h-96 inset-y-0">
                    <!-- Item 1 -->
                    <div class="hidden duration-700 ease-in-out" data-carousel-item>
                        <div class="flex flex-wrap justify-center gap-4 sm:gap-6">
                            <img src={ASU_logo.src} class="block max-w-xs" alt="ASU">
                            <img src={GEN_SPACE_logo.src} class="block max-w-xs" alt="GEN SPACE">
                            <img src={CEDIA_logo.src} class="block max-w-xs" alt="CEDIA">
                            <img src={MOVISTAR_logo.src} class="block max-w-xs" alt="MOVISTAR">
                        </div>
                    </div>
                    <!-- Item 2 -->
                    <div class="hidden duration-700 ease-in-out" data-carousel-item>
                        <div class="flex flex-wrap justify-center gap-4 sm:gap-6">
                            <img src={MINISTERIO_E_logo.src} class="block max-w-lg" alt="MINISTERIO E">
                            <img src={MINISTERIO_PCEIP_logo.src} class="block max-w-lg" alt="MINISTERIO PCEIP">
                            <img src={MINISTERIO_REMH_logo.src} class="block max-w-lg" alt="MINISTERIO REMH">
                        </div>
                    </div>
                    <!-- Item 3 -->
                    <div class="hidden duration-700 ease-in-out" data-carousel-item>
                        <div class="flex flex-wrap justify-center gap-4 sm:gap-6">
                            <img src={GUAYAQUIL_SPACE_logo.src} class="block max-w-xs" alt="GUAYAQUIL SPACE">
                            <img src={ECUACAP_logo.src} class="block max-w-xs" alt="ECUACAP">
                            <img src={BUENAVISTA_logo.src} class="block max-w-xs" alt="BUENAVISTA">
                        </div>
                    </div>
                    <!-- Item 4 -->
                    <div class="hidden duration-700 ease-in-out" data-carousel-item>
                        <div class="flex flex-wrap justify-center gap-4 sm:gap-6">
                            <img src={WORKINGUP_logo.src} class="block max-w-xs" alt="WORKINGUP">
                            <img src={AMCHAM_logo.src} class="block max-w-xs" alt="AMCHAM">
                            <img src={AEI_logo.src} class="block max-w-xs" alt="AEI">
                        </div>
                    </div>
                    <!-- Item 5 -->
                    <div class="hidden duration-700 ease-in-out" data-carousel-item>
                        <div class="flex flex-wrap justify-center gap-4 sm:gap-6">
                            <img src={USFQ_logo.src} class="block max-w-xs" alt="USFQ">
                            <img src={LEVIATHAN_SPACE_logo.src} class="block max-w-xs" alt="LEVIATHAN SPACE">
                        </div>
                    </div>
                </div>
                <!-- Slider indicators -->
                <div class="absolute z-30 flex -translate-x-1/2 bottom-5 left-1/2 space-x-3 rtl:space-x-reverse">
                    <button type="button" class="w-3 h-3 rounded-full bg-black/70" aria-current="true" aria-label="Slide 1" data-carousel-slide-to="0"></button>
                    <button type="button" class="w-3 h-3 rounded-full bg-black/70" aria-current="false" aria-label="Slide 2" data-carousel-slide-to="1"></button>
                    <button type="button" class="w-3 h-3 rounded-full bg-black/70" aria-current="false" aria-label="Slide 3" data-carousel-slide-to="2"></button>
                    <button type="button" class="w-3 h-3 rounded-full bg-black/70" aria-current="false" aria-label="Slide 4" data-carousel-slide-to="3"></button>
                    <button type="button" class="w-3 h-3 rounded-full bg-black/70" aria-current="false" aria-label="Slide 5" data-carousel-slide-to="4"></button>
                </div>
            </div>
        </div>
    </section>
    <script src="https://unpkg.com/flowbite@1.4.1/dist/flowbite.js"></script>
</body>
